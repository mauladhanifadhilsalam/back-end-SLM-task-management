meta {
  name: List Notifications
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/notifications?recipientId=&targetType=&targetId=&state=UNREAD
  body: none
  auth: inherit
}

params:query {
  recipientId: 1
  targetType: TICKET
  targetId: 1
  state: UNREAD
  page: 1
  pageSize: 20
  sortOrder: asc
  status: IN_PROGRESS
  sentFrom: 2024-01-01
  sentTo: 2024-12-31
  sortBy: id
}

settings {
  encodeUrl: true
}
